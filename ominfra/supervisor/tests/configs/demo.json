{
  "nodaemon": true,
  "groups": {
    "default": {
      "processes": {
        "sleep": {
          "command": "sleep 600",
          "stdout": {
            "file": "/dev/fd/1",
            "maxbytes": 0
          },
          "redirect_stderr": true
        },
        "ls": {
          "command": "ls -al",
          "stdout": {
            "file": "/dev/fd/1",
            "maxbytes": 0
          },
          "redirect_stderr": true
        }
      }
    }
  }
}
